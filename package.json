{
  "dependencies": {
    "torch-lock": "https://github.com/PR0M3TH3AN/TORCH/archive/refs/heads/main.tar.gz"
  },
  "scripts": {
    "torch:dashboard": "npm run --prefix torch dashboard:serve",
    "torch:check": "npm run --prefix torch lock:check:daily",
